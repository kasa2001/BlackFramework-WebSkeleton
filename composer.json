{
  "name": "blackframework/web-skeleton",
  "type": "project",
  "license": "MIT",
  "description": "Skeleton for web sites",
  "minimum-stability": "stable",
  "require": {
    "blackframework/core": "*",
    "blackframework/flex": "*",
    "blackframework/routing": "*"
  },
  "require-dev": {
  },
  "config": {
    "allow-plugins": {
      "blackframework/*": true
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}